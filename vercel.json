{
    "version": 2,
    "builds": [
      { "src": "api/*.rs", "use": "@vercel/rust" },
      { "src": "src/**/*", "use": "@vercel/static-build", "config": { "distDir": "build" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  